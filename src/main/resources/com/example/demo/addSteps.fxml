<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AddStepsController">
   <children>
      <TableView fx:id="table" layoutX="294.0" layoutY="80.0" prefHeight="338.0" prefWidth="469.0">
         <columns>
            <TableColumn fx:id="nameCol" prefWidth="243.0" text="Name" />
            <TableColumn fx:id="descriptionCol" prefWidth="222.0" text="Description" />
         </columns>
      </TableView>
      <Button layoutX="180.0" layoutY="384.0" mnemonicParsing="false" onAction="#deleteStep" prefHeight="31.0" prefWidth="82.0" text="Delete" />
      <Button layoutX="57.0" layoutY="384.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#addStep" prefHeight="31.0" prefWidth="82.0" text="Insert" />
      <Label layoutX="60.0" layoutY="116.0" text="Name" />
      <Label layoutX="60.0" layoutY="176.0" text="Description" />
      <TextField fx:id="name" layoutX="58.0" layoutY="137.0" prefHeight="31.0" prefWidth="203.0" />
      <TextArea fx:id="description" layoutX="58.0" layoutY="197.0" prefHeight="175.0" prefWidth="203.0" />
      <Label fx:id="planningName" layoutX="58.0" layoutY="31.0" text="Planning name" wrapText="true">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
